<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC   
    "-//mybatis.org//DTD Mapper 3.0//EN"  
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="Dao.Cart1Dao">

<select id="searchByUserId" resultType="Entity.Cart1">
	select * from cart1 where user_id=#{user_id} 


</select>
<update id="updateZongji">
   update cart1 set zongji=#{zongji} where id=#{cart1_id}

</update>



</mapper> 